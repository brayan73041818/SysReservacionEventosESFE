@model SysReservacionEventosESFE.EntidadesDeNegocio.RolAccesos

@{
    ViewData["Title"] = "Delete";
}

<h1>Delete</h1>

<h3>¿ Esta seguro que desea eliminar ?</h3>
<div>
    <h4>Este acceso</h4>
    <hr />
    <dl class="row">
       
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Rol)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Rol.Nombre)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Accesos)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Accesos.Nombre)
        </dd>

    </dl>
    @using (Html.BeginForm("Delete", "RolAccesos", new { IdRolAcceso = Model.IdRolAcceso }, FormMethod.Post))
    {
        @Html.AntiForgeryToken()

        <button type="submit" class="btn btn-danger" onclick="return confirm('¿Está seguro de eliminar?');" style="margin-left: 10px;">
            <i class="fas fa-trash"></i> Eliminar
        </button>

        <button type="button" class="btn btn-secondary" onclick="window.location.href='@Url.Action("Reportes")'" style="margin-left: 10px;">
            <i class="fas fa-times"></i> Cancelar
        </button>
        <span class="text-danger">@ViewBag.Error</span>
    }
</div>

